	<h1 id="firstHeading" class="firstHeading" lang="br">Henroma</h1>
	
	<div id="bodyContent" class="mw-body-content">
		<div id="siteSub" class="noprint">Eus Wikipedia</div>
		<div id="contentSub"></div>
		
		
		<div id="jump-to-nav"></div>
		<a class="mw-jump-link" href="Henroma#mw-head">Sauter à la navigation</a>
		<a class="mw-jump-link" href="Henroma#p-search">Sauter à la recherche</a>
		<div id="mw-content-text" lang="br" dir="ltr" class="mw-content-ltr"><div class="mw-parser-output"><table class="WSerieV" id="WRome_antique" cellspacing="0" cellpadding="0" border="0" align="right" style="margin: 0em 0em 1em 1em; background-color:#f7f8ff; padding:5px; border:1px solid #8888aa; border-right-width: 2px; border-bottom-width: 2px; line-height: 1.5em; text-align:center">
