      <section class="foot">
        <h2 class="product-logo">
          <a href="./" target="_self"              aria-label="Google"
              
      data-g-category="footer"
      data-g-action="navigation"
      data-g-label="Logo"
        data-g-href="/"
>
                <svg alt="Google"  class="logo-svg " xmlns="http://www.w3.org/2000/svg" viewBox="0 0 86 28">
