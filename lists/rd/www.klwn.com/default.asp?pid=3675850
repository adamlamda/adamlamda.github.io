		var thetarget = arguments[0];

		var sessionUserID = "";
		sessionUserID = sessionUserID * 1;
		if (sessionUserID > 0){
			var xmlhttp = new XMLHttpRequest();
			xmlhttp.open('POST', 'trklnk.asp', true);

			xmlhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
			xmlhttp.send('thelink=' + thelink +'&station=klwn-am&id='+ sessionUserID);
		}

		var pageTracker;
		
			try {
				pageTracker=_gat._getTracker("UA-57792900-1");
