    <script type="text/javascript">
        function clearDeadRSSlinks() {
            try {
                var links = document.body.getElementsByTagName('a');
                var urlHere = new String(document.location.href);
                var i, link, evalStr, parentNode, childNode;

                for (i = links.length - 1; i >= 0; i--) {
                    link = links[i];
                    evalStr = new String(link.href);

                    if (evalStr.length === 0 || urlHere.indexOf(evalStr.toString()) !== -1) {
                        evalStr = new String(link.className);

                        if (evalStr.toString() === 'fd_itemlink') {
                            evalStr = link.text;
                            childNode = document.createElement('span');
                            childNode.innerHTML = evalStr;
                            childNode.style.fontWeight = 'bold';
                            parentNode = link.parentNode;
                            parentNode.replaceChild(childNode, link);
                        }
                    }
                }
            }
            catch (ex) { }
        }
        clearDeadRSSlinks();
    </script>
