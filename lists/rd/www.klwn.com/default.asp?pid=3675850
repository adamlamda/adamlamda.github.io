	function openpsa(theval)
	{
		if(document.getElementById){
			if (document.getElementById('lorm'+theval).innerHTML =="MORE"){
				document.getElementById('lorm'+theval).innerHTML ="LESS";
				document.getElementById('prow'+theval).style.display ="block";
			}else{
				document.getElementById('lorm'+theval).innerHTML ="MORE";
				document.getElementById('prow'+theval).style.display ="none";
			}
		}
