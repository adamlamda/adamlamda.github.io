<script id="scrptBlog" type="text/javascript">

    function isNull(evalItem) {
        // Note: var undefined may need to go away eventually as 'undefined' is 
        // a reserved work for the ECMAScript version 5 specification.  As of 26 Oct 2011
        // ECMAScript version 5 is not currently readily adopted by the most common web browser products.
        var undefined;
        if (evalItem === null || typeof (evalItem) === 'undefined' || evalItem === undefined) {
            return true;
        }
        else {
            return false;
        }
    }

    
    function rollOver(imageElementName, imageRolloverUrl) {
        var imgs, img, i, evalName, el;
        try {
            if (imageElementName.length === 0 || imageRolloverUrl.length === 0) {
                return;
            }
            imageElementName = new String(imageElementName);
            var imgs = document.getElementsByTagName('img');
            for (var i = imgs.length - 1; i > -1; i--) {
                img = imgs[i];
                evalName = new String(img.getAttribute('name'));
                if (evalName.length > 0) {
                    if (evalName.toLowerCase() === imageElementName.toLowerCase()) {
                        img.src = imageRolloverUrl;
                        return;
                    }
                }
            }

            // just in case...
            el = document.getElementById(imageElementName);
            if (isNull(el) === false && isNull(el.src) === false) {
                el.src = imageRolloverUrl;
            }
        }
        catch (ex) { }
    }

    function checkYear(year) {
        return (((year % 4 == 0) && (year % 100 != 0)) || (year % 400 == 0)) ? 1 : 0;
    }
    var today = new Date();
    var themonthv = today.getMonth();
    if ((themonthv == 0) || (themonthv == 2) || (themonthv == 4) || (themonthv == 6) || (themonthv == 7) || (themonthv == 9) || (themonthv == 11)) {
        var maxLength = 31;
    } else if ((themonthv == 3) || (themonthv == 5) || (themonthv == 8) || (themonthv == 10)) {
        var maxLength = 30;
    } else if ((themonthv == 1)) {
        if (checkYear(today.getYear()) == 0) {
            var maxLength = 28;
        } else {
            var maxLength = 29;
        }
    }


    function isInternetExplorer() {
        var agnt = navigator.userAgent.toLowerCase();
        var ie = !!(agnt.indexOf('msie') != -1);
        return ie;
    }

    function openlinks(theval, oorc, thelen) {
        // the 1 below we can get from the navneed variable
        var ie = isInternetExplorer();
        var el;
        var el2;
        // the 1 below we can get from the navneed variable
        for (var zyx = 0; zyx < thelen; zyx++) {
            el = document.getElementById('prow' + theval + zyx);
            el2 = document.getElementById('tr' + theval + zyx);
            if (isNull(el) === false && isNull(el2) === false) {
                if (oorc == "O") {
                    el.style.visibility = "visible";
                    el2.style.visibility = "visible";
                    if (ie == true) {
                        el.style.position = "fixed";
                        el2.style.position = "fixed";
                    } else {
                        el.style.height = "100%";
                    }
                } else {
                    el.style.visibility = "hidden";
                    el2.style.visibility = "hidden";
                    if (ie == true) {
                        el.style.position = "absolute";
                        el2.style.position = "absolute";
                    } else {
                        el.style.height = "0px";
                    }
                }
            }
        }
    }

    function hideElement(elName) {
        var el = document.getElementById(elName);
        if (isNull(el) === false) {
            el.style.zIndex = '0';
            el.style.display = 'none';
        }
    }

    function showElement(elName) {
        var el = document.getElementById(elName);
        if (isNull(el) === false) {
            el.style.zIndex = 500;
            el.style.display = 'block';
        }
    }
</script>
