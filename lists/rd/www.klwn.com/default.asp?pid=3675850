			if (thetarget =="_blank"){
				window.open(thelink, 'goolink','width='+ winW +',height='+ winH +',status=yes,scrollbars=yes,resizable=yes,toolbar=yes,location=yes');
			}else{
				setTimeout('document.location = "' + thelink + '"', 100);
			}
			
	}
