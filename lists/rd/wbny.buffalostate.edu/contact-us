child.eq(1).show();


$('#pg-menu-content ul li.active').show().parent().show();

$('#pg-menu-content ul li .plus').click(function(){
	$(this).parent().addClass('active active-trail');
	$(this).hide();
	$(this).siblings('.minus').show();
	$('#pg-menu-content ul li.active ul').slideDown();
});
$('#pg-menu-content ul li .minus').click(function(){
	$(this).parent().removeClass('active active-trail');
	$(this).hide();
	$(this).siblings('.plus').show();
	$(this).siblings('ul').slideUp();
	});
});



