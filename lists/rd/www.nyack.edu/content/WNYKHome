<script>
function toggleSearch(id) {
       var e = document.getElementById(id);
       if(e.style.display == 'block') {
          e.style.display = 'none';
		  document.getElementById('searchIcon').src = '/images/2015/assets/search.png';
	   } else {
          e.style.display = 'block';
		  $('#terms').focus();
		  document.getElementById('searchIcon').src = '/images/2015/assets/search2.png';
	   }
    }
	
function toggleSearchMobile(id) {
       var e = document.getElementById(id);
       if(e.style.display == 'block') {
          e.style.display = 'none';
	   } else {
          e.style.display = 'block';
		  $('#terms-MOBILE').focus();
	   }
    }
	
function toggleNav(id) {
       var e = document.getElementById(id);
       if(e.style.display == 'block') {
          e.style.display = 'none';
	   } else {
          e.style.display = 'block';
		  $('#terms').focus();
	   }
    }

//$('#terms').focus();
</script>

