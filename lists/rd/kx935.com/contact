<script>
jQuery(document).ready(function(){
	jQuery('.minus').hide();

    //jQuery(".parent_menu_ul").click(function(){
	   //jQuery(this).parent().next(".parent_menu_ul_ul").slideToggle("slow");
	   //jQuery('.parent_menu_ul span').attr('class','minus');
	   //jQuery('.plus').hide();
	   //jQuery('.minus').show();
    //});
					     
    jQuery('div.dropdown').each(function() {
        var $dropdown = jQuery(this);

        jQuery(".parent_menu_ul", $dropdown).click(function(e) {
           e.preventDefault();
          $div = jQuery(".parent_menu_ul_ul", $dropdown);
          
          $div.toggle();
          
          //jQuery(".plus").not($div).hide();
          jQuery(".parent_menu_ul_ul").not($div).hide();
          return false;
        });

    });

    jQuery('.plus').click(function () {
    	var par = jQuery(this);
    	var plustext = par.text();
    	if(plustext == '+'){
    		jQuery('.plus').text('+');
    		par.text('–');  
    	}else if(plustext == '–'){
    		jQuery('.plus').text('+');		        	
    		par.text('+');  
    	}
    });

    jQuery('.entertainment').on('click', function () {
        jQuery('html, body').animate({
        scrollTop: (jQuery("#entertainment").offset().top) - 120
        }, 1000);
    });
    
    jQuery('.givingback').on('click', function () {
        jQuery('html, body').animate({
        scrollTop: (jQuery("#givingback").offset().top) - 120
        }, 1000);
    });
    
    jQuery('.parking').on('click', function () {
        jQuery('html, body').animate({
        scrollTop: (jQuery("#parking").offset().top) - 120
        }, 1000);
    });
    
    jQuery('.credit-footer-link').attr('target','_blank');

    jQuery(".event-post-description a").each(function(){
        var valuea = jQuery(this).attr('href');

        if(valuea.indexOf('https://itunes.apple.com/us') > -1) {
          jQuery(this).addClass('itunes-btn');
          jQuery(this).attr('target', '_blank');
        }
        
    });

    /* episodes count download */
    jQuery(document).on('click','.download-podcast-ctn',function() {
        var el = jQuery(this);
        var pId = jQuery(el).parents('.post-item').find('.get-post-id').attr('id');            
        var ajaxurl = 'http://www.kx935.com/wp-admin/admin-ajax.php';
        data = { action: 'podcastsdownload', podcastsDownloadID: pId};
        jQuery.ajax({
            url: ajaxurl,
            data: data,
            type: 'post',
            success: function(response) {
                //console.log(response);
                jQuery(el).parents('.post-item').find('.totaldownloadcount').text(response);
            }
        });
    });
    /* episodes count download */  
});
</script>
</body>
</html>