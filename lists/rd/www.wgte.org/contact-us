        var episodeQs = "?episode_id=";
        if ( window.location.search.substr(0, episodeQs.length ) == episodeQs ) {
            var episodeId = window.location.search.replace(episodeQs, "");
            var episodeAnchor = "#episode_id_" + episodeId;
            var scrollHeight = Math.round( jQuery(episodeAnchor).offset().top ) - 80;
            jQuery("html,body").animate({ scrollTop: scrollHeight }, "slow");
        }
        
