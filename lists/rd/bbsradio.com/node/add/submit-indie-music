  <label class="element-invisible" for="edit-field-24-website-und-8-weight">Weight for row 9 </label>
 <select class="field_24_website-delta-order form-select" id="edit-field-24-website-und-8-weight" name="field_24_website[und][8][_weight]"><option value="-8">-8</option><option value="-7">-7</option><option value="-6">-6</option><option value="-5">-5</option><option value="-4">-4</option><option value="-3">-3</option><option value="-2">-2</option><option value="-1">-1</option><option value="0">0</option><option value="1">1</option><option value="2">2</option><option value="3">3</option><option value="4">4</option><option value="5">5</option><option value="6">6</option><option value="7">7</option><option value="8" selected="selected">8</option></select>
