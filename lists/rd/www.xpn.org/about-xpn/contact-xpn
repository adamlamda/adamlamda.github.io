function clickAudit1(data1,data2)
{
	//alert(document.window.location.href);
	var myurl = document.location.origin+"/static/counter.php";
	 $.ajax({
       type: "POST",
       url: myurl,
       data: "var1="+data1+"&var2="+data2+"&var3="+document.window.location.href,
	   error: function(msg){
		 console.log(msg);  
	   },
       success: function(msg){
        // alert( "Data Saved: " + msg ); //Anything you want
		window.location.reload(true);
       }
     });
}
/*jQuery.noConflict();*/
</script>
