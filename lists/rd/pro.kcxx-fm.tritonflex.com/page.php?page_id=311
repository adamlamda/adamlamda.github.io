var keywordSearch = new function() {
	this.defaultMessage = "Enter Keyword";
	this.searchMessage = "Please Wait...";
	this.notfoundMessage = "Keyword Not Found";
	this.keyword = "";
	this.box = null;

	this.search = function() {
		(function($) {
			$.post('/common/ajax.php', {t:'keyword', xaction:'search',keyword: document.getElementById('keyword_search').value},
			function (str) {
				switch(str) {
					case "not found":
						document.getElementById('keyword_search').value = "Keyword Not Found";
						break;
					case "no search":
						document.getElementById('keyword_search').value = keywordSearch.notfoundMessage;
						break;
					default:
						document.location.href=str;
						break;
				}
			});
		})(jQuery);
		keywordSearch.keyword = document.getElementById('keyword_search').value;
		document.getElementById('keyword_search').value = this.searchMessage;
		return false;
	};

	this.checkChar = function(evt) {
		var charCode = (evt.which) ? evt.which : event.keyCode;
		if(charCode == 13) {
			keywordSearch.search();
		}
	}
}



</script><form class="keyword_form" id="keyword_form" onsubmit="return false"><input onkeyup="keywordSearch.checkChar(event)" onclick="if(this.value==keywordSearch.defaultMessage || this.value==keywordSearch.notfoundMessage) this.value='';" type="text" id="keyword_search" value="Enter Keyword" /><div id="keyword_button" onclick="keywordSearch.search()"><input type="button" value="GO" /></div></form></div></div><div class=" box-3041-29"><div class="module"><div>
