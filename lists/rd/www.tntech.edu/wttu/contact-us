        $(window).on('load', function(){
            //resize logo on scroll
            var shrinkHeight = $("#standard>header").height() - 120;
            $(window).on("scroll", function(){
            	if($(window).scrollTop() > shrinkHeight) $("#mainav").addClass("shrink");
            	else $("#mainav").removeClass("shrink");
            });
        });
        
