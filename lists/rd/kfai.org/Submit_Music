<script>
	$(window).load(function(){
		$('.sticky-enabled tr').each(function() {
			var old = parseInt($(this).find('.field-name-field-duration .field-item').html());
			var minutes = Math.floor(old/60);
			var seconds = old % 60;
			if(seconds < 10)
				seconds = "0" + seconds.toString();
			$(this).find('.field-name-field-duration .field-item').html(minutes + ':' + seconds);
		});
	});
	
//Form Submit
$('#webform-client-form-42820').bind('submit', function (event) {
	var error_msg = "", error_status = 0;
	$("#error-message").html("");
	$("#error-message").hide();
	var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
	if($("#edit-submitted-first-name").val().length < 2) {
		error_msg = error_msg + "First name is required<br/>";
		error_status = 1;
	}
	if($("#edit-submitted-last-name").val().length < 2) {
		error_msg = error_msg + "Last name is required<br/>";
		error_status = 1;
	}
    if($("#edit-submitted-phone-number").val().length < 9) {
		error_msg = error_msg + "Phone Number must be of length 9 or more<br/>";
		error_status = 1;
	}
	if(!emailReg.test($("#edit-submitted-email-address").val())) {
		error_msg = error_msg + "Email address entered is invalid<br/>";
		error_status = 1;
	}
	if($("#edit-submitted-address").val().length < 2) {
		error_msg = error_msg + "Address is required<br/>";
		error_status = 1;
	}
	if($("#edit-submitted-city").val().length < 2) {
		error_msg = error_msg + "City is required<br/>";
		error_status = 1;
	}
	if($("#edit-submitted-state").val() == "") {
		error_msg = error_msg + "Please select a state<br/>";
		error_status = 1;
	}
	if($("#edit-submitted-zip").val().length < 5) {
		error_msg = error_msg + "Zip code must be of length 5<br/>";
		error_status = 1;
	}
	if(error_status == 1) {
		$("#error-message").html(error_msg);
		$("#error-message").show();
		return false;
	}
	});
</script>
