		<script>
            function CookieSaveClick() {

                $("#cookieBanner").fadeOut();
                setCookieAcceptance();

                if (getTrackingCookie() == "") {
                    // user has closed the banner while declining to interact with the Analytics toggle, implicitly refusing those cookies.
                    // we have an obligation to ensure any GA cookies still on their computer from a previous session are cleared.
                    clearGoogleAnalyticsCookies();
                }

                event.preventDefault();
                event.stopPropagation();
                return false;
            }

            function CookieCogClick() {

                $("#cookieBanner").fadeToggle();

                event.preventDefault();
                event.stopPropagation();
                return false;
            }

			function MobileIsLive() {
				return true;
			}

			function SiteIsEnglish() {
				var link = $('#englishLink')[0];
				if (link.style.fontWeight == "bold") return true;

				return false;
			}

			function BrowserIsMobile() {

				if (navigator.userAgent.match(/Android/i)
					|| navigator.userAgent.match(/webOS/i)
					|| navigator.userAgent.match(/iPhone/i)
					|| navigator.userAgent.match(/iPad/i)
					|| navigator.userAgent.match(/iPod/i)
					|| navigator.userAgent.match(/BlackBerry/i)
					|| navigator.userAgent.match(/Windows Phone/i)
				) {
					return true;
				}
				else {
					return false;
				}
            }

            function setTrackingCookie(val) {

                if (val != "1")
                    val = "0";

                var d = new Date();
                d.setMonth(d.getMonth() + 1);
                var expires = "expires=" + d.toUTCString();
                document.cookie = "lw.com.allow-tracking=" + val + ";secure;samesite=lax;path=/;" + expires;
                                
                if (val == "0") {
                    // user has explicitly toggles Off the Analytics cookies. clear 'em out.
                    clearGoogleAnalyticsCookies();                    
                }
            }

            function clearGoogleAnalyticsCookies() {
                // clear any remaining google analytics cookies
                // note this does not work in dev or staging, but should work in production 
                // this is due to the way google's scripts set the domain on the cookies to .lw.com, which can be modified from www.lw.com but not from any other subdomain
                clearCookie("_ga");
                clearCookie("_gat");
                clearCookie("_gid");
                clearCookie("_gat_gtag_UA_81474352_4"); // spanish
                clearCookie("_gat_gtag_UA_81474352_5"); // italian
                clearCookie("_gat_gtag_UA_81474352_6"); // japanese
                clearCookie("_gat_gtag_UA_81474352_7"); // korean
                clearCookie("_gat_gtag_UA_81474352_8"); // russian
            }

            function clearCookie(name) {
                var d = new Date();
                d.setFullYear(2000); // expire immediately
                var expires = "expires=" + d.toUTCString();
                document.cookie = name + "=;path=/;expires=" + expires;
            }

            function getTrackingCookie() {
                var cookieArray = document.cookie.split(';');
                for (i = 0; i < cookieArray.length; i++) {
                    var cookie = cookieArray[i];
                    while (cookie.charAt(0) == ' ') {
                        cookie = cookie.substring(1);
                    }
                    if (cookie.indexOf("lw.com.allow-tracking=") == 0) {
                        return cookie.substring(22, cookie.length);
                    }
                }
                return "";
            }

			function setCookieAcceptance() {
				var d = new Date();
				d.setMonth(d.getMonth() + 1);
				var expires = "expires=" + d.toUTCString();
				document.cookie = "lw.com.cookie-acceptance=1;secure;samesite=lax;path=/;" + expires;
			}

			function getCookieAcceptance() {

				var cookieArray = document.cookie.split(';');
				for (i = 0; i < cookieArray.length; i++) {
					var cookie = cookieArray[i];
					while (cookie.charAt(0) == ' ') {
						cookie = cookie.substring(1);
					}
					if (cookie.indexOf("lw.com.cookie-acceptance=") == 0) {
						return cookie.substring(25, cookie.length);
					}
				}
				return "";
			}

			function setMobilePreference(preference) {
				var d = new Date();
				d.setMonth(d.getMonth() + 1);
				var expires = "expires=" + d.toUTCString();
                document.cookie = "m.lw.com.mobile-preference=" + preference + ";secure;samesite=lax;path=/" + expires;
			}

			function getMobilePreference() {

				var cookieArray = document.cookie.split(';');
				for (i = 0; i < cookieArray.length; i++) {
					var cookie = cookieArray[i];
					while (cookie.charAt(0) == ' ') {
						cookie = cookie.substring(1);
					}
					if (cookie.indexOf("m.lw.com.mobile-preference=") == 0) {
						return cookie.substring(27, cookie.length);
					}
				}

				return "";
			}

            $(document).ready(function () {

                var allowTracking = getTrackingCookie();
                if (allowTracking == "1")
                    $("#cbAllowTracking").prop("checked", true);

                $('#cbAllowTracking').change(function () {
                    if (this.checked) {
                        setTrackingCookie("1");
                    } else {
                        setTrackingCookie("2");
                    }
                });

				$('#PersonalInfo a[href*="mailto:"], #ContactList a[href*="mailto:"], .businessCardInfo a[href*="mailto:"], #EmailLabel a[href*="mailto:"]').on('click', function (event) {
					event.preventDefault();
					$('#overlay').fadeIn('fast');
					$('#email-disclaimer-button-continue').prop('href', this.href);
				});
				$('#email-disclaimer-button-continue').on('click', function (event) {
					event.preventDefault();
					var loc = $(this).prop('href');
					window.location = loc;
					$('#overlay').fadeOut('fast');
				});
				$('#email-disclaimer-button-cancel, #overlay').on('click', function () {
					$('#overlay').fadeOut('fast');
					return false;
				});
				
				// hide the footer mobile link and the pipe separator ( "|" ) next to it
				$('.linkToMobile').hide().next().hide();

				if (MobileIsLive() && BrowserIsMobile() && SiteIsEnglish()) {

					// show the link to the mobile site, and make sure that
					// clicking it sets the preference cookie to mobile
					$('.linkToMobile').show().next().show();
					$('.linkToMobile').click(function () {
						setMobilePreference("mobile");
					});
                }

				if (getCookieAcceptance() == "") {
                    $('#cookieBanner').show();
                }

                $("a.youtube-preview").hover(
                    function () {
                        $(this).find("span.youtube-icon-mimic img.red").fadeIn(110);
                    },
                    function () {
                        $(this).find("span.youtube-icon-mimic img.red").fadeOut(110);
                    });

                $("a.vimeo-preview").hover(
                    function () {
                        $(this).find("span.youtube-icon-mimic img.red").fadeIn(110);
                    },
                    function () {
                        $(this).find("span.youtube-icon-mimic img.red").fadeOut(110);
                    });

                // user tried to click the preview - show the consent overlay
                $("a.youtube-preview").click(function (evt) {

                    var key = $(this).data("videokey");
                    var consent = $("div.youtube-consent[data-videokey=" + key + "]");

                    consent.fadeIn(200);                    

                    evt.preventDefault();
                    evt.stopPropagation();
                    return false;

                });
                $("a.vimeo-preview").click(function (evt) {

                    var key = $(this).data("videokey");
                    var consent = $("div.vimeo-consent[data-videokey=" + key + "]");

                    consent.fadeIn(200);

                    evt.preventDefault();
                    evt.stopPropagation();
                    return false;

                });

                // consent overlay 'Decline' button clicked
                $("button.youtubeConsentDecline").click(function (evt) {

                    var key = $(this).data("videokey");                    
                    var consent = $("div.youtube-consent[data-videokey=" + key + "]");

                    consent.fadeOut(200);

                    evt.preventDefault();
                    evt.stopPropagation();
                    return false;

                });
                $("button.vimeoConsentDecline").click(function (evt) {

                    var key = $(this).data("videokey");
                    var consent = $("div.vimeo-consent[data-videokey=" + key + "]");

                    consent.fadeOut(200);

                    evt.preventDefault();
                    evt.stopPropagation();
                    return false;

                });

                // consent overlay 'Accept' button clicked
                $("button.youtubeConsentAccept").click(function (evt) {

                    var key = $(this).data("videokey");
                    var preview = $("a.youtube-preview[data-videokey=" + key + "]");
                    var consent = $("div.youtube-consent[data-videokey=" + key + "]");

                    // generate the embedded youtube video iframe
                    var iframe = "<iframe width='384' height='216' src='https://www.youtube.com/embed/" + key + "?rel=0&amp;showinfo=0' frameborder='0' allow='autoplay; encrypted-media'></iframe>";

                    // hide the preview and consent overlay, and inject the iframe
                    preview.hide();
                    consent.hide();
                    consent.after(iframe);

                    evt.preventDefault();
                    evt.stopPropagation();
                    return false;

                });
                $("button.vimeoConsentAccept").click(function (evt) {

                    var key = $(this).data("videokey");
                    var preview = $("a.vimeo-preview[data-videokey=" + key + "]");
                    var consent = $("div.vimeo-consent[data-videokey=" + key + "]");

                    // generate the embedded vimeo video iframe
                    var iframe = "<iframe width='384' height='216' src='https://player.vimeo.com/video/" + key + "' frameborder='0'></iframe>";

                    // hide the preview and consent overlay, and inject the iframe
                    preview.hide();
                    consent.hide();
                    consent.after(iframe);

                    evt.preventDefault();
                    evt.stopPropagation();
                    return false;

                });

                // any p tag containing a video preview should remove all margins
                $("p").has(".youtube-preview").css("margin", "0");
                $("p").has(".vimeo-preview").css("margin", "0");
            });

		</script>
		<div id="overlay">
			<div id="email-disclaimer">

				<div id="zoneEmailDisclaimer" class="editableZoneFrameNoEdit">
	<div class="editableZoneContent">
		<span style="font-family: Arial, sans-serif; font-size: 10pt;"><strong>Notice:</strong></span><span style="font-family: Arial, sans-serif; font-size: 10pt;"> We appreciate your interest in Latham &amp; Watkins. If your inquiry relates to a legal matter and you are not already a current client of the firm, please do not transmit any confidential information to us.&nbsp;Before taking on a representation, we must determine whether we are in a position to assist you and agree on the terms and conditions of engagement with you. Until we have completed such steps, we will not be deemed to have a lawyer-client relationship with you, and will have no duty to keep confidential the information we receive from you. Thank you for your understanding.</span>
	</div>
</div>
				<div id="email-disclaimer-button-container">
					<a href="andrew-prins#" id="email-disclaimer-button-cancel">Cancel</a>
					<a href="andrew-prins#" id="email-disclaimer-button-continue">Accept</a>
				</div>
			</div>
		</div>		
	

