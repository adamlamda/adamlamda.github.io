    <div id="BusinessCardPanel">
        <h1 id="ContentPlaceHolder1_HeadingPlaceHolder_NameLabel">Andrew D. Prins</h1>
        <ul id="PersonalInfo">
            <li><span id="ContentPlaceHolder1_HeadingPlaceHolder_TitleLabel" class="bioTitle">Partner</span></li>
            <li id="ContentPlaceHolder1_HeadingPlaceHolder_EmailListItem"><span class="addressLegend"><span class="multilanguage" SiteTranslationName="$MinisiteLabel_BusinessCardEmailIndicator">E</span></span><a href="mailto:andrew.prins@lw.com" id="ContentPlaceHolder1_HeadingPlaceHolder_EmailLink">andrew.prins@lw.com</a></li>
            <li id="PhoneNumberListItem"><span class="addressLegend"><span class="multilanguage" SiteTranslationName="$MinisiteLabel_BusinessCardTelephoneIndicator">T</span></span><span id="PhoneNumberLabel">+1.202.637.3317</span></li>
            
            <li id="ContentPlaceHolder1_HeadingPlaceHolder_AddContactListItem"><a id="AddToContactsLink" href="https://www.lw.com/VCardHandler.ashx?empid=71848"><span class="multilanguage" SiteTranslationName="$MinisiteLabel_AddToContacts">Add to contacts</span></a></li>
